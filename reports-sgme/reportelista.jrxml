		</parameter>
		<queryString>
			<![CDATA[SELECT
    so.*,
    soar.*,
    ar.*,
    us.*,
    soar.estado AS estado_solicitud_articulos,
soar.id AS id_solicitud_aulasyarticulos
FROM
    sgme.solicitudes so
INNER JOIN
    sgme.solicitudes_articulos soar
    ON so.id = soar.id_solicitudes
INNER JOIN
    sgme.articulos ar
    ON ar.id = soar.id_articulos
INNER JOIN
    usuarios us
    ON us.id = so.id_usuario
where ar.id = 61]]>
		</queryString>
